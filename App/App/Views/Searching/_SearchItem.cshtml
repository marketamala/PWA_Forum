@model App.Controllers.SearchModel

@using (Html.BeginForm())
{
<div class="form-group" style="margin-bottom: 15px; margin-left: 15px">
    @Html.EditorFor(model => model.search, new { htmlAttributes = new { @class = "form-control", @style = "display: inline-block; min-width: 40%; width: 40%" } })
    <input type="submit" value="Hledat" class="btn btn-default" style="display:inline-block; width: 13%; background-color: black; color:white"/> 
</div>
}

